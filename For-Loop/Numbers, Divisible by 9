function solve(firstNumber, secondNumber) {
    let a = Number(firstNumber);
    let b = Number(secondNumber);
    let sum = 0;
    let x = "";
    for (i = a; i <= b; i++) {
        if ((i % 9) == 0) {
            sum = sum + i;
        }
    }
    console.log(`The sum: ${sum}`);

    for (i = a; i <= b; i++) {
        if ((i % 9) == 0) {
            x += i + " ";
        }
    }
    console.log(`${x} `);
}

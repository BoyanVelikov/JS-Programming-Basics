function vowelSum(n, chars) {
    let numero = Number(n);
    let sum = 0;
    for (i = 1; i <= numero; i++) {
        let letter = chars[i - 1];
        if (letter === "a") {
            sum = sum + 1;
        } else if (letter === "e") {
            sum = sum + 2;
        } else if (letter === "i") {
            sum = sum + 3;
        } else if (letter === "o") {
            sum = sum + 4;
        } else if (letter === "u") {
            sum = sum + 5;
        }
    }
    console.log(sum);
}

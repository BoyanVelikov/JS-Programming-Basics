function numberSequence(n, numbers) {
    let a = Number(n);
    let maxNumber = -Infinity;
    let minNumber = Infinity;

    for (i = 1; i <= a; i++) {
        let c = numbers[i - 1];
        let d = numbers[i - 1];
        if (c >= maxNumber) {
            maxNumber = c;
        }
        if (d <= minNumber) {
            minNumber = d;
        }
    }
    console.log(`Max number: ${maxNumber}`);
    console.log(`Min number: ${minNumber}`);
}
